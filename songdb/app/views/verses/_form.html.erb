<%= simple_form_for @verse, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :song_id %>
  <%= f.input :order %>
  <%= f.input :verse_type_id %>
  <%= f.input :note %>
  <%= f.input :content %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                verses_path, :class => 'btn' %>
  </div>
<% end %>
